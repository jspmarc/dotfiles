[subway-bw]
# main_fg is the left bar and floating bar that shows up when you like a song
main_fg                               = ${xrdb:color0:#0a0e10}

# secondary_fg is the font color
secondary_fg                          = ${xrdb:color0:#0a0e10}

# main_bg is the bg of the main content (artist page, bottom row, etc)
main_bg                               = ${xrdb:color7:#dcdfe2}

# dropdown settings menu, friends etc
sidebar_and_player_bg                 = ${xrdb:color7:#dcdfe2}

cover_overlay_and_shadow              = ${xrdb:color7:#dcdfe2}

# The bottom bar when offline or downloading something 
# + the play button on covers 
# + the play button on other pages 
# + the speaker symbol bg on the album/playlist playing
indicator_fg_and_button_bg            = ${xrdb:color6:#bac5cd}

# The background to album/playlist/etc download button when downloaded
sidebar_indicator_and_hover_button_bg = ${xrdb:color1:#7a8382}

# Scrollbar
scrollbar_fg_and_selected_row_bg      = ${xrdb:color9:#7a8382}

# The home, radio, etc icon on the sidebar
preserve_1                            = ${xrdb:color7:#dcdfe2}

# I don't know the rest
